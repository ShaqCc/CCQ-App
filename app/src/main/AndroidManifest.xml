        <activity android:name=".ui.user.LoginActivity" />
        <activity android:name=".ui.publish.BaseMapActivity" />
        <activity android:name=".ui.publish.BrandModelSelectActivity" />
        <activity android:name=".ui.user.UserSubscribeActivity" />
        <activity android:name=".ui.message.SingleChatActivity"/>

